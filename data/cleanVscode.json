{
	"app": {
		"settings": ["protoc"],
		"launch": [2, 1]
	},
	"server": {
		"settings": [
			"protoc",
			{ "files.exclude": "app/out/**/*.map" },
			"path-intellisense.mappings",
			"eslint.workingDirectories",
			"prettier.configPath",
			"[javascript]",
			"[typescript]"
