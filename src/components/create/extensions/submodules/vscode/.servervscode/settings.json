{
	"clang-format.style": false,
	"files.exclude": {
		"**/.submodule": true,
		"**/.github": true,
		"**/.husky/_": true,
		"**/.husky/.gitignore": true
	},
	"commitlint.config.file": "${workspaceRoot}/hooks/commitlint.config.js",
	"protoc": {
		"compile_on_save": false,
		"options": ["--proto_path=${workspaceRoot}/proto", "--proto_path=/usr/include/google/protobuf", "--proto_path=/usr/local/include/google/protobuf"]
	},
	"cSpell.words": [
		"Assemblyscript",
		"MINVER",
		"Proto",
		"Uncapitalize",
		"assemblyscript",
		"binaryen",
		"commitlint",
		"corejs",
		"etag",
		"grpc",
		"grpcweb",
		"lookpath",
		"nodelib",
		"pinia",
		"protoc",
		"toplevel",
		"unmount",
		"vuex",
		"wasm",
		"webassembly",
		"webpackbar"
	]
}
